<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SingUpController">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="389.0">
         <children>
            <ImageView fx:id="brandingImageView" fitHeight="600.0" fitWidth="418.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/LewaStrona.jpg" />
               </image>
            </ImageView>
         </children></AnchorPane>
      <AnchorPane prefHeight="590.0" prefWidth="584.0">
         <children>
            <ImageView fx:id="lockImageView" fitHeight="228.0" fitWidth="398.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/LOGO_HCH.jpg" />
               </image>
            </ImageView>
            <TextField fx:id="tf_username" layoutX="136.0" layoutY="260.0" prefHeight="40.0" prefWidth="194.0" promptText="Twój login">
               <font>
                  <Font name="Arial" size="12.0" />
               </font></TextField>
            <PasswordField fx:id="tf_password" layoutX="136.0" layoutY="322.0" prefHeight="40.0" prefWidth="194.0" promptText="Twoje hasło">
               <font>
                  <Font name="Arial" size="12.0" />
               </font></PasswordField>
            <Label layoutX="22.0" layoutY="260.0" prefHeight="40.0" prefWidth="94.0" text="Login">
               <font>
                  <Font name="Arial" size="17.0" />
               </font>
            </Label>
            <Label layoutX="22.0" layoutY="322.0" prefHeight="40.0" prefWidth="94.0" text="Hasło">
               <font>
                  <Font name="Arial" size="17.0" />
               </font>
            </Label>
            <Button fx:id="button_signup" layoutX="23.0" layoutY="434.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="132.0" style="-fx-background-color: #b9aeae;" text="Zarejestruj się" textFill="#922222">
               <font>
                  <Font name="Arial" size="17.0" />
               </font>
            </Button>
            <Button fx:id="button_log_in" layoutX="206.0" layoutY="434.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="132.0" style="-fx-background-color: #b9aeae;" text="Zaloguj się" textFill="#922222">
               <font>
                  <Font name="Arial" size="17.0" />
               </font>
            </Button>
         </children></AnchorPane>
   </children>
</HBox>
